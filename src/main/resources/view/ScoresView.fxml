<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="me.ryan.controller.ScoresController"
      maxHeight="Infinity" maxWidth="Infinity"
      minHeight="100.0" minWidth="600.0"
      prefHeight="100.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.141">

    <padding>
        <Insets right="10" left="10"/>
    </padding>
    <children>
        <TableView fx:id="scoresTable"
                   minHeight="100" minWidth="75">
            <columns>
                <TableColumn fx:id="cpm" text="速度"/>
                <TableColumn fx:id="kps" text="击键"/>
                <TableColumn fx:id="keysEachChar" text="码长"/>

                <TableColumn fx:id="timeInterval" text="用时"/>
                <TableColumn fx:id="keyEnterCount" text="回车"/>
                <TableColumn fx:id="typos" text="错字"/>
                <TableColumn fx:id="charactersCount" text="字数"/>
                <TableColumn fx:id="ratioOfWords" text="打词"/>
                <TableColumn fx:id="backspaceCount" text="退格"/>
                <TableColumn fx:id="keystrokes" text="键数"/>
                <TableColumn fx:id="keysAccuracy" text="键准"/>
                <TableColumn fx:id="retypeCount" text="重打"/>
            </columns>
        </TableView>
    </children>
</VBox>
